"""Print sample fake data to STDOUT."""


from __future__ import print_function, absolute_import

import sys
import os

sys.path.insert(0, os.path.abspath(f'{os.path.dirname(__file__)}/../src/libs'))
sys.path.insert(0, os.path.abspath(f'{os.path.dirname(__file__)}/../src'))


def main():
    """Print sample fake data."""
    from workflow import Workflow
    import fakeum

    wf = Workflow()
    fakeum.wf = wf
    print('| Name | Example |')
    print('|-- |--|')
    for name in fakeum.FAKERS:
        ex = fakeum.get_fake_datum(name)
        print(f'| {name} | {ex} |'.encode('utf-8'))


if __name__ == '__main__':
    sys.exit(main())
